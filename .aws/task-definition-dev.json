{
  "family": "compath-deploy-task",
  "containerDefinitions": [
    {
      "name": "compath-api",
      "image": "008971659095.dkr.ecr.ap-northeast-2.amazonaws.com/compath-ecr:2024-08-02T04-03-02",
      "cpu": 0,
      "portMappings": [
        {
          "name": "compath-server-port",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/compath-deploy-task",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "executionRoleArn": "arn:aws:iam::008971659095:role/ecsTaskExecutionRole",
  "networkMode": "bridge",
  "cpu": "512",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
